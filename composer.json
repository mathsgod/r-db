{
    "name": "mathsgod/r-db",
    "description": "A lightweight orm library for mysql",
    "autoload": {
        "psr-4": {
            "R\\DB\\": "src"
        },
        "files": [
            "function.php"
        ]
    },
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*",
        "mathsgod/data-structure": "^1.3",
        "mathsgod/php-util": "^1.5",
        "laminas/laminas-db": "^2.12",
        "laminas/laminas-hydrator": "^4.2",
        "psr/event-dispatcher": "^1.0",
        "symfony/validator": "^5.3",
        "doctrine/annotations": "^1.13",
        "league/event": "^3.0",
        "symfony/cache": "^5 | ^6.0",
        "laminas/laminas-paginator": "^2.11",
        "vlucas/phpdotenv": "^5.4",
        "psr/http-message": "^1.0",
        "laminas/laminas-di": "^3.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "thecodingmachine/graphqlite": "^5.0 | ^6.0"
    },
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Raymond Chong"
        }
    ],
    "scripts": {
        "test": "phpunit --bootstrap tests\\Testing.php tests",
        "test81": "php81 ./vendor/bin/phpunit --bootstrap tests\\Testing.php tests",
    }
}
